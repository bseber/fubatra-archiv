<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style>
		.newGroup {
			margin-top: 1em;
		}
	</ui:style>
	
	<g:HTMLPanel>
		<g:CheckBox ui:field="cbBallcontrol" text="Ballmitnahme" />
		<g:SimplePanel>
			<g:VerticalPanel width="100%">
				<g:CheckBox ui:field="cbDribbling" text="Dribbling" />
				<g:CheckBox ui:field="cbFeints" text="Finten" />
				<g:CheckBox ui:field="cbHeader" text="Kopfball" />
				<g:CheckBox ui:field="cbPassing" text="Passspiel" />
				<g:CheckBox ui:field="cbShoot" text="Torschuss" />
			</g:VerticalPanel>
		</g:SimplePanel>
		<g:SimplePanel styleName="{style.newGroup}">
			<g:VerticalPanel width="100%">
				<g:CheckBox ui:field="cbOffIndividual" text="Angriff Individualtaktik" />
				<g:CheckBox ui:field="cbOffGroup" text="Angriff Gruppentaktik" />
				<g:CheckBox ui:field="cbOffTeam" text="Angriff Mannschaftstaktik" />
			</g:VerticalPanel>
		</g:SimplePanel>
		<g:SimplePanel styleName="{style.newGroup}">
			<g:VerticalPanel width="100%">
				<g:CheckBox ui:field="cbDefIndividual" text="Abwehr Individualtaktik" />
				<g:CheckBox ui:field="cbDefGroup" text="Abwehr Gruppentaktik" />
				<g:CheckBox ui:field="cbDefTeam" text="Abwehr Mannschaftstaktik" />
			</g:VerticalPanel>
		</g:SimplePanel>
		<g:SimplePanel styleName="{style.newGroup}">
			<g:VerticalPanel width="100%">
				<g:CheckBox ui:field="cbStamina" text="Ausdauer" />
				<g:CheckBox ui:field="cbCoordination" text="Koordination" />
				<g:CheckBox ui:field="cbPower" text="Kraft" />
				<g:CheckBox ui:field="cbSpeed" text="Schnelligkeit" />
			</g:VerticalPanel>
		</g:SimplePanel>
	</g:HTMLPanel>
	
</ui:UiBinder> 